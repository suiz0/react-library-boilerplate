{
  "name": "react-library-boilerplate",
  "version": "0.1.0",
  "description": "A boilerplate project to create react components",
  "main": "./build/my-lib.bundle.js",
  "scripts": {
    "build": "rimraf build/** && npm run types:build && webpack --config webpack.prod.config.js",
    "build:dev": "webpack --config webpack.dev.config.js",
    "types:build": "tsc",
    "start": "webpack serve --config webpack.dev.config.js --open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suiz0/react-library-boilerplate.git"
  },
