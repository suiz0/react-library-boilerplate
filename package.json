  "bugs": {
    "url": "https://github.com/suiz0/react-library-boilerplate/issues"
  },
  "homepage": "https://github.com/suiz0/react-library-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.7.3",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "rimraf": "^5.0.0",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
